.h2w-darkblue {
  color:#ddd;
  background-color:#353a51;
}

/**标题**/
.h2w-darkblue .h2w__h1,
.h2w-darkblue .h2w__h2 {
    border-color:#626c96;
}


/**表格**/
.h2w-darkblue .h2w__thead .h2w__tr {
    background-color:#626c96;
}
.h2w-darkblue .h2w__table .h2w__tr:nth-child(2n){
    background-color:#414763;
}
.h2w-darkblue .h2w__th,
.h2w-darkblue .h2w__td {
    border-color:#626c96;
}


/**代码块**/
.h2w-darkblue .h2w__pre,
.h2w-darkblue .h2w__pre .h2w__code {
    background-color:#626c96;
    border-color:#626c96;
}

.h2w-darkblue .h2w__code {
    background-color:#272822;
    border-color:#1b1c18;
}


/**块元素**/
.h2w-darkblue .h2w__blockquote {
    border-left-color:#626c96;
}

/**内连元素**/
.h2w-darkblue .h2w__a {
    color:#1aad16; border-color:#4d804b;
}

.h2w-darkblue .h2w__hr {
    background-color:#626c96;
}

.h2w-darkblue .h2w__mark {
    background:yellow;
    color:#7683b4;
}

.h2w-darkblue .h2w__todoCheckbox .wx-checkbox-input {
    background:#626c96; border-color:#626c96;
}
.h2w-darkblue .h2w__todoCheckbox .wx-checkbox-input.wx-checkbox-input-checked {
    background:green; border-color:#4d804b;
}
.h2w-darkblue .h2w__todoCheckbox .wx-checkbox-input.wx-checkbox-input-checked::before {
    color:white;
}

/**代码高亮样式**/
@import '/towxml/plugins/hljs/style/monokai.wxss';
